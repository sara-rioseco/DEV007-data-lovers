  },
  "dependencies": {
    "@types/jest": "^29.5.11",
    "firebase": "^9.22.1"
  },
  "resolutions": {
    "jest-environment-jsdom": "^27"
  },
  "jest": {
    "testEnvironment": "jsdom"
  }
}
